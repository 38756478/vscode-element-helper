{
    "name": "vscode-element-helper",
    "displayName": "vscode-element-helper",
    "description": "a vscode extension for Element-UI",
    "version": "0.0.6",
    "publisher": "ElemeFE",
    "repository": {
        "type": "git",
        "url": "https://github.com/ElemeFE/vscode-element-helper"
    },
    "engines": {
        "vscode": "^1.12.0"
    },
    "categories": [
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onLanguage:html",
        "onLanguage:vue",
        "onCommand:element-helper.searchUnderCursor"
    ],
    "main": "./out/src/extension",
    "icon": "element.png",
    "contributes": {
        "commands": [
            {
                "command": "element-helper.searchUnderCursor",
                "title": "element-helper.search"
            }
        ],
        "keybindings": [
            {
                "command": "element-helper.searchUnderCursor",
                "key": "ctrl+cmd+z",
                "when": "editorTextFocus"
            }
        ],
        "snippets": [
            {
                "language": "javascript",
                "path": "./snippets/element.json"
            }
        ],
        "configuration": {
            "type": "object",
            "title": "ELement Helper Configuration",
            "properties": {
                "element-helper.language": {
                    "type": "string",
                    "default": "zh-CN",
                    "description": "Document Language of Element UI",
                    "enum": [
                        "zh-CN",
                        "en-US"
                    ]
                },
                "element-helper.version": {
                    "type": "string",
                    "default": "1.3",
                    "description": "Dcument version of Element UI",
                    "enum": [
                        "1.0",
                        "1.1",
                        "1.2",
                        "1.3"
                    ]
                }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.1",
        "element-helper-json": "^1.0.0",
        "follow-redirects": "^1.2.3",
        "jquery": "^3.2.1",
        "mkdirp": "^0.5.0",
        "pretty": "^2.0.0"
    }
}